from typing import *


def maximumAggregateCount(n: int, a: List[int]) -> int:
        temp1=a[0:n-1]
        temp2=a[1:n]
        if(sum(temp1)>sum(temp2)):
            return sum(temp1)
        else:
            return sum(temp2)
            

class Solution {
    public String generateTheString(int n) {
        if(n==1) return "x";
        String ans = "";
        if(n%2==0){
            
            ans+="a";
            for(int i=0;i<n-1;i++){
                ans+="b";
            }

           
        }
        else{
            ans+="a";
            n=n-1;
            ans+="b";
            for(int i=0;i<n-1;i++){
                ans+="c";
            }

            
        }
        return ans;
    }
}
